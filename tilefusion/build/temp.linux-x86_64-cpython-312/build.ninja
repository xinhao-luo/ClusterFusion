ninja_required_version = 1.3
cxx = c++
nvcc = /root/cuda-12.4/bin/nvcc

cflags = -pthread -B /root/miniconda3/envs/sglang/compiler_compat -fno-strict-overflow -DNDEBUG -O2 -Wall -fPIC -O2 -isystem /root/miniconda3/envs/sglang/include -fPIC -O2 -isystem /root/miniconda3/envs/sglang/include -fPIC -I/root/miniconda3/envs/sglang/lib/python3.12/site-packages/torch/include -I/root/miniconda3/envs/sglang/lib/python3.12/site-packages/torch/include/torch/csrc/api/include -I/root/miniconda3/envs/sglang/lib/python3.12/site-packages/torch/include/TH -I/root/miniconda3/envs/sglang/lib/python3.12/site-packages/torch/include/THC -I/root/cuda-12.4/include -I/root/miniconda3/envs/sglang/include/python3.12 -c
post_cflags = -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=tilefusion -D_GLIBCXX_USE_CXX11_ABI=0 -std=c++17
cuda_cflags = -I/root/miniconda3/envs/sglang/lib/python3.12/site-packages/torch/include -I/root/miniconda3/envs/sglang/lib/python3.12/site-packages/torch/include/torch/csrc/api/include -I/root/miniconda3/envs/sglang/lib/python3.12/site-packages/torch/include/TH -I/root/miniconda3/envs/sglang/lib/python3.12/site-packages/torch/include/THC -I/root/cuda-12.4/include -I/root/miniconda3/envs/sglang/include/python3.12 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -O3 -std=c++17 -arch=compute_90a -code=sm_90a -lcuda -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=tilefusion -D_GLIBCXX_USE_CXX11_ABI=0
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc --generate-dependencies-with-compile --dependency-output $out.d $cuda_cflags -c $in -o $out $cuda_post_cflags





build /cephfs/xhluo/Fuse-all-Kernel/tilefusion/build/temp.linux-x86_64-cpython-312/kernel/llm_decoder_layer.o: cuda_compile /cephfs/xhluo/Fuse-all-Kernel/tilefusion/kernel/llm_decoder_layer.cu
build /cephfs/xhluo/Fuse-all-Kernel/tilefusion/build/temp.linux-x86_64-cpython-312/pybind.o: compile /cephfs/xhluo/Fuse-all-Kernel/tilefusion/pybind.cpp






